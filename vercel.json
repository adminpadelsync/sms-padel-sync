{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/feedback",
            "schedule": "*/10 * * * *"
        },
        {
            "path": "/api/cron/invite-timeout",
            "schedule": "*/5 * * * *"
        },
        {
            "path": "/api/cron/recalculate-scores",
            "schedule": "0 3 * * *"
        },
        {
            "path": "/api/cron/result-nudges",
            "schedule": "*/30 * * * *"
        }
    ]
}